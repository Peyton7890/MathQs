{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "src/app/**/*.tsx",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/main.py"
      }
    ]
  }
  