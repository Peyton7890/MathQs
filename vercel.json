{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/generate",
        "dest": "api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "src/app/page.tsx"
      }
    ]
  }
  